<% present(nil, WelcomePresenter) do |w| %>
	<div class="gap-y-2 py-6 place-content-center border-b border-gray-900/10">
	  <h1 class="mb-4 text-2xl font-extrabold"><%= w.welcome %></h1>

		<div class="max-w-full justify-items-center">
		  <h3><pre class="font-mono font-semibold"><%= w.total_dues_current %></pre></h3>
		  <h3><pre class="font-mono font-semibold"><%= w.total_dues_last_year %></pre></h3>	    
	  </div>

		<div class="max-w-full">....</div>

		<div class="text-xl font-bold"><%= w.board_title %></div>

		<div>
			<%= 
        render "shared/table", 
            head: 'current_positions_head', 
            item_row: 'current_positions_row', 
            collection: w.current_positions 
      %>
		</div>
		
	</div>
<% end %>
